algoritmo "Aula 7 - IMC"
// Função : Exercicio aula 7
// Autor :
// Data : 19/07/2022
// Seção de Declarações 
var
  M, A, IMC: Real
inicio
      Escreva ("Massa (KG): ")
      Leia (M)
      Escreva ("Altura (m): ")
      Leia (A)
      IMC <- M / (A ^ 2)
      Escreval ("IMC: ", IMC)
      
fimalgoritmo
